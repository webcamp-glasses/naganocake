<div class="container">
  <div class="row">
    <div class="col-lg-12 mt-5 mb-5">
      <%= @end_user.last_name %> <%= @end_user.first_name %> さんの会員情報編集
    </div>
    <div class="row">
    <%= form_with model: @end_user, url:admins_end_user_path, local: true  do |f| %>
      <div>
        <%= f.label :"会員ID", class: "col-sm-3 control-label" %>
        <%= f.label :"#{@end_user.id}" %>
      </div>
      <div>
        <%= f.label :"氏名", class: "col-sm-3 control-label" %>
        <%= f.text_field :last_name, class: "col-sm-3" %>
        <%= f.text_field :first_name, class: "col-sm-3" %>
      </div>
      <div>
        <%= f.label :"フリガナ", class: "col-sm-3 control-label" %>
        <%= f.text_field :last_kana_name, class: "col-sm-3" %>
        <%= f.text_field :first_kana_name, class: "col-sm-3" %>
      </div>
      <div>
        <%= f.label :"郵便番号", class: "col-sm-3 control-label" %>
        <%= f.text_field :postal_code, class: "col-sm-3" %>
      </div>
      <div>
        <%= f.label :"住所", class: "col-sm-3 control-label" %>
        <%= f.text_field :address, class: "col-sm-3" %>
      </div>
      <div>
        <%= f.label :"電話番号", class: "col-sm-3 control-label" %>
        <%= f.text_field :phone_number, class: "col-sm-3" %>
      </div>
      <div>
        <%= f.label :"メールアドレス", class: "col-sm-3 control-label" %>
        <%= f.text_field :email, class: "col-sm-3" %>
      </div>
      <div>
        <%= f.label :"会員ステータス", class: "col-sm-3 control-label" %>
        <%#= f.radio_button :is_deleted, true %>
        <%#= f.radio_button :is_deleted, false %>
        <%#= f.label :is_deleted, "有効", {value: :false, style: "display: inline-block;"} %>
      </div>
      <div class="col-sm-4">
        <%= f.submit "変更を保存", class: "btn btn-success" %>
      </div>
　　<% end %>
　　</div>
　</div>
　
  </div>
</div>