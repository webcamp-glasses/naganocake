 <%= form_with model: genre, :url => { :action => :create} do |f| %>

    <div class="form-group">
     <%= f.label :ジャンル %>
     <%= f.text_field :genre, placeholder: "ジャンル名" %>
    </div>
    
    <div class="form-group">
      <%= f.submit "新規登録", class: "btn btn-success" %>
    </div>
<% end %>
